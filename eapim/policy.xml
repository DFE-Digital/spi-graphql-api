<policies>

    <inbound>

        <base />
      
        <set-header name="X-Functions-Key" exists-action="skip">
            <value>{{graph-function-app-key}}</value>
        </set-header>

        <rate-limit calls="30" renewal-period="60" />

    </inbound>

    <backend>
        <base />
    </backend>

    <outbound>
        <base />
    </outbound>

    <on-error>
        <base />
    </on-error>

</policies>